{
  "include": [
    "src/**/*.ts", 
    "src/**/*.tsx",
    "updateAddresses/**/*.ts"
  ],
  "compilerOptions": {
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": ["es2015", "dom"],
    "rootDir": "./",
    "types": ["googlemaps", "node" ]
  }
}

